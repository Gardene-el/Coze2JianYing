# API 调用: create_video_segment
# 时间: {time.strftime('%Y-%m-%d %H:%M:%S')}

        # 构造 request 对象参数列表
        params = []
        if args.input.material_url is not None:
            params.append(f"material_url="https://gardene-el.github.io/Coze2JianYing/assets/video.mp4"")
        if args.input.target_timerange is not None:
            # 转换 CustomNamespace 为 TimeRange
            obj = args.input.target_timerange
            if hasattr(obj, '__dict__'):
                obj_params = ', '.join(f"{k}={v}" for k, v in vars(obj).items())
            else:
                obj_params = str(obj)
            params.append(f"target_timerange=TimeRange({obj_params})")
        if args.input.source_timerange is not None:
            # 转换 CustomNamespace 为 TimeRange
            obj = args.input.source_timerange
            if hasattr(obj, '__dict__'):
                obj_params = ', '.join(f"{k}={v}" for k, v in vars(obj).items())
            else:
                obj_params = str(obj)
            params.append(f"source_timerange=TimeRange({obj_params})")
        if args.input.speed is not None:
            params.append(f"speed=None")
        if args.input.volume is not None:
            params.append(f"volume=None")
        if args.input.change_pitch is not None:
            params.append(f"change_pitch=None")
        if args.input.clip_settings is not None:
            # 转换 CustomNamespace 为 ClipSettings
            obj = args.input.clip_settings
            if hasattr(obj, '__dict__'):
                obj_params = ', '.join(f"{k}={v}" for k, v in vars(obj).items())
            else:
                obj_params = str(obj)
            params.append(f"clip_settings=ClipSettings({obj_params})")

# 构造 request 对象
req_{generated_uuid} = CreateVideoSegmentRequest({", ".join(params)})

resp_{generated_uuid} = await create_video_segment(req_{generated_uuid})

segment_65e0ed0a_ece3_45dd_8127_0593e68aee46 = resp_65e0ed0a_ece3_45dd_8127_0593e68aee46.segment_id



